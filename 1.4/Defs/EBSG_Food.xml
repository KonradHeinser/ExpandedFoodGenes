<Defs>
    <GeneDef>
        <defName>EBSG_ResistantStomach</defName>
        <label>resistant stomach</label>
        <description>Carriers of this gene have an extra toxin-filtering organ in their stomach that makes it a lot more durable against potential food poisoning.</description>
        <iconPath>UI/Icons/Genes/Gene_StrongStomach</iconPath>
        <foodPoisoningChanceFactor>0.33</foodPoisoningChanceFactor>
        <biostatMet>-2</biostatMet>
        <displayCategory>EBSG_Food</displayCategory>
        <displayOrderInCategory>8.9</displayOrderInCategory>
        <symbolPack>
            <prefixSymbols>
                <li><symbol>dura</symbol></li>
                <li><symbol>eat</symbol></li>
                <li><symbol>feed</symbol></li>
                <li><symbol>cram</symbol></li>
            </prefixSymbols>
            <suffixSymbols>
                <li><symbol>vore</symbol></li>
            </suffixSymbols>
        </symbolPack>
        <exclusionTags>
            <li>EBSG_StrongStomachs</li>
        </exclusionTags>
    </GeneDef>

